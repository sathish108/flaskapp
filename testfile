pipeline {
    agent any
    stages {
        stage ('pulling code from git hub') {
            steps {
                git credentialsId: 'github_credentials', url: 'https://github.com/sathish108/flaskapp'    
		        stash 'Source'
            }    
        }
        stage ('second stage') {
            steps {
                echo "hi sathish"
            }           
        }
    }
}